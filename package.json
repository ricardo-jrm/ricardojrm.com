    "@material-ui/icons": "^4.5.1",
    "@types/core-js": "^2.5.2",
    "@types/next": "^8.0.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "core-js": "^3.2.1",
    "next": "^9.0.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "typescript": "^3.6.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "@typescript-eslint/parser": "^2.1.0",
